#include <iostream>
#include <cmath>
using namespace std;

int main() {
    int a, b, c;
    cout << "Enter a, b, c: ";
    cin >> a >> b >> c;

    double D = (b * b) - (4 * a * c);

    if (D < 0) {
        cout << "No real roots";
    } else {
        double root1 = (-b + sqrt(D)) / (2 * a);
        double root2 = (-b - sqrt(D)) / (2 * a);

        cout << "Root1: " << root1 << '\n';
        cout << "Root2: " << root2 << endl;
    }

    return 0;
}

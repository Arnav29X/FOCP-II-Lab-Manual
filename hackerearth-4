#include <iostream>
#include <cctype>
using namespace std;
int main() {
    char str[100];
    int i = 0;

	cin >> str;
    while (str[i] != '\0') {
        if (str[i] >= 'A' && str[i] <= 'Z') str[i] = tolower(str[i]);
		else str[i] = toupper(str[i]);
        i++;
    }

    cout << str;
    return 0;
}

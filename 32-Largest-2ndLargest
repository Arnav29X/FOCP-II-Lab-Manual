#include <iostream>
using namespace std;

int main() {
    int arr[5];

    cout << "---- ENTER ELEMENTS ----\n";
    for (int i = 0; i < 5; i++) {
        cout << "Enter number " << i + 1 << ": ";
        cin >> arr[i];
    }

    int max, second_largest;

    if (arr[0] > arr[1]) {
        max = arr[0];
        second_largest = arr[1];
    } else {
        max = arr[1];
        second_largest = arr[0];
    }

    for (int i = 2; i < 5; i++) {
        if (arr[i] > max) {
            second_largest = max;
            max = arr[i];
        }
        else if (arr[i] > second_largest && arr[i] != max) {
            second_largest = arr[i];
        }
    }

    cout << "\nMAX: " << max;
    cout << "\nSECOND LARGEST: " << second_largest;

    return 0;
}

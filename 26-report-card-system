#include <iostream>
using namespace std;

int main() {
    int x;
    float marks[5];
    float total, percent;

    cout << "How many students? : ";
    cin >> x;

    for (int i = 0; i < x; i++) {
        total = 0;

        cout << "\nEnter marks for STUDENT " << i + 1 << endl;

        for (int j = 0; j < 5; j++) {
            cout << "Enter marks " << j + 1 << ": ";
            cin >> marks[j];
            total += marks[j];
        }

        percent = total / 5;

        cout << "\n--- RESULT ---" << endl;
        cout << "Total Marks: " << total << endl;
        cout << "Percentage: " << percent << "%" << endl;
    }

    return 0;
}

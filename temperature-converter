// CONVERSION OF FAHRENHEIT TO CELSIUS

#include <iostream>
using namespace std;

int main(){
    double fahrenheit;

    cout << "Enter temperature in fahrenheit: ";
    cin >> fahrenheit;

    double celsius = (fahrenheit - 32.0) * 5.0 / 9.0;
    cout << "Fahrenheit to Celsius: " << celsius;

    return 0;
}

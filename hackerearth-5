#include <iostream>
using namespace std;

int main() {
    int L, N;
	cin >> L;
	cin >> N;

    for (int i = 0; i < N; i++) {
        int W, H;
		cin >> W >> H;

        if (W < L || H < L) {
            printf("UPLOAD ANOTHER\n");
        } else if (W == H) {
            printf("ACCEPTED\n");
        } else {
            printf("CROP IT\n");
        }
    }
    return 0;
}
